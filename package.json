{
    "name": "oracle-cpq-bml-snippets",
    "displayName": "Developer Tools for CPQ",
    "description": "A collection of developer tools and code snippets to speed up coding in Oracle CPQ.",
    "version": "0.0.29",
    "publisher": "YashwanthKumar",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yashwanth2714/Oracle-CPQ-BML-Snippets"
    },
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.86.0"
    },
    "scripts": {
        "minify": "node scripts/minify.js",
        "package": "node scripts/package.js"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:bml",
        "onLanguage:plaintext",
        "onLanguage:java",
        "onLanguage:c"
    ],
    "categories": [
        "Snippets",
        "Programming Languages",
        "Formatters",
        "Themes"
    ],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "cpqExplorer",
                    "title": "CPQ",
                    "icon": "images/activityBarIcon.svg"
                }
            ]
        },
        "views": {
            "cpqExplorer": [
                {
                    "id": "cpqDocs",
                    "name": "BML Functions",
                    "icon": "images/activityBarIcon.svg",
                    "type": "webview"
                }
            ]
        },
        "languages": [
            {
                "id": "bml",
                "aliases": [
                    "BML",
                    "bml"
                ],
                "extensions": [
                    ".bml"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "dark": "images/bml.svg",
                    "light": "images/bml.svg"
                }
            }
        ],
        "grammars": [
            {
                "language": "bml",
                "scopeName": "source.java",
                "path": "./syntaxes/bml.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "bml",
                "path": "snippets/snippets.json"
            },
            {
                "language": "plaintext",
                "path": "snippets/snippets.json"
            },
            {
                "language": "c",
                "path": "snippets/snippets.json"
            },
            {
                "language": "java",
                "path": "snippets/snippets.json"
            }
        ],
        "configurationDefaults": {
            "[bml]": {
                "editor.defaultFormatter": "YashwanthKumar.oracle-cpq-bml-snippets"
            }
        },
        "themes": [
            {
                "label": "Dracula (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/dracula.json"
            },
            {
                "label": "One Dark Pro (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/one-dark-pro.json"
            },
            {
                "label": "Solarized Dark (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/solarized-dark.json"
            },
            {
                "label": "Solarized Light (CPQ)",
                "uiTheme": "vs",
                "path": "./themes/solarized-light.json"
            },
            {
                "label": "Monokai (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/monokai.json"
            },
            {
                "label": "Gruvbox Dark (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/gruvbox-dark.json"
            },
            {
                "label": "Night Owl (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/night-owl.json"
            },
            {
                "label": "Palenight (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/palenight.json"
            },
            {
                "label": "Tomorrow Night Blue (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/tomorrow-night-blue.json"
            },
            {
                "label": "Dark+ (CPQ Custom)",
                "uiTheme": "vs-dark",
                "path": "./themes/dark-plus.json"
            },
            {
                "label": "GitHub Dark Dimmed (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/github-dark-dimmed.json"
            },
            {
                "label": "GitHub Light (CPQ)",
                "uiTheme": "vs",
                "path": "./themes/github-light.json"
            },
            {
                "label": "Ayu Mirage (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/ayu-mirage.json"
            },
            {
                "label": "Ayu Light (CPQ)",
                "uiTheme": "vs",
                "path": "./themes/ayu-light.json"
            },
            {
                "label": "Nord (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/nord.json"
            },
            {
                "label": "Quiet Light (CPQ)",
                "uiTheme": "vs",
                "path": "./themes/quiet-light.json"
            },
            {
                "label": "Bluloco Light (CPQ)",
                "uiTheme": "vs",
                "path": "./themes/bluloco-light.json"
            },
            {
                "label": "Notepad++ Light (CPQ)",
                "uiTheme": "vs",
                "path": "./themes/notepadpp-light.json"
            },
            {
                "label": "Notepad++ Obsidian (CPQ)",
                "uiTheme": "vs-dark",
                "path": "./themes/notepadpp-obsidian.json"
            }
        ]
    },
    "galleryBanner": {
        "color": "#1F4761",
        "theme": "dark"
    },
    "keywords": [
        "developer tools for cpq",
        "bml coding tools",
        "oracle cpq coding snippets",
        "bml coding snippets",
        "oracle cpq bml snippets",
        "bml snippets",
        "cpq",
