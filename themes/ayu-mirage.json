{
    "$schema": "vscode://schemas/color-theme",
    "name": "Ayu Mirage (CPQ)",
    "type": "dark",
    "colors": {
        "editor.background": "#1f2430",
        "editor.foreground": "#cbccc6",
        "editorCursor.foreground": "#ffcc66",
        "editor.lineHighlightBackground": "#191e2a",
        "editor.selectionBackground": "#33415e"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#5c6773"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#bae67e"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#ffa759"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#5ccfe6"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#ffd580"
            }
        }
    ]
}