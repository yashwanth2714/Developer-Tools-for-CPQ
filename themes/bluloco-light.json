{
    "$schema": "vscode://schemas/color-theme",
    "name": "Bluloco Light (CPQ)",
    "type": "light",
    "colors": {
        "editor.background": "#f0f0f0",
        "editor.foreground": "#404040",
        "editorCursor.foreground": "#aeafad",
        "editor.lineHighlightBackground": "#e0e0e0",
        "editor.selectionBackground": "#c5d9f7"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#8e908c"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#718c00"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#8959a8"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#4271ae"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#f5871f"
            }
        }
    ]
}