{
    "$schema": "vscode://schemas/color-theme",
    "name": "Dracula (CPQ)",
    "type": "dark",
    "colors": {
        "editor.background": "#282a36",
        "editor.foreground": "#f8f8f2",
        "editor.selectionBackground": "#44475a",
        "editor.lineHighlightBackground": "#44475a80",
        "editorCursor.foreground": "#f8f8f0",
        "editorWhitespace.foreground": "#6272a4"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#6272a4",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#f1fa8c"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#ff79c6"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#50fa7b"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#bd93f9"
            }
        }
    ]
}