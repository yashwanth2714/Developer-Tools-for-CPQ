{
    "$schema": "vscode://schemas/color-theme",
    "name": "GitHub Dark Dimmed (CPQ)",
    "type": "dark",
    "colors": {
        "editor.background": "#22272e",
        "editor.foreground": "#adbac7",
        "editorCursor.foreground": "#cdd9e5",
        "editor.lineHighlightBackground": "#2d333b",
        "editor.selectionBackground": "#3392ff44"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#768390",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#96d0ff"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#f47067"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#dcbdfb"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#6cb6ff"
            }
        }
    ]
}