{
    "$schema": "vscode://schemas/color-theme",
    "name": "GitHub Light (CPQ)",
    "type": "light",
    "colors": {
        "editor.background": "#ffffff",
        "editor.foreground": "#24292e",
        "editorCursor.foreground": "#044289",
        "editor.lineHighlightBackground": "#f6f8fa",
        "editor.selectionBackground": "#0366d625"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#6a737d",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#032f62"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#d73a49"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#005cc5"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#6f42c1"
            }
        }
    ]
}