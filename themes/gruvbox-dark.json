{
    "$schema": "vscode://schemas/color-theme",
    "name": "Gruvbox Dark (CPQ)",
    "type": "dark",
    "colors": {
        "editor.background": "#282828",
        "editor.foreground": "#ebdbb2",
        "editorCursor.foreground": "#fe8019",
        "editor.lineHighlightBackground": "#3c3836",
        "editor.selectionBackground": "#504945"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#928374"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#b8bb26"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#fb4934"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#fabd2f"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#83a598"
            }
        }
    ]
}