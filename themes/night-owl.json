{
    "$schema": "vscode://schemas/color-theme",
    "name": "Night Owl (CPQ)",
    "type": "dark",
    "colors": {
        "editor.background": "#011627",
        "editor.foreground": "#d6deeb",
        "editorCursor.foreground": "#d6deeb",
        "editor.lineHighlightBackground": "#2a2a2a",
        "editor.selectionBackground": "#1d3b53"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#637777"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#ecc48d"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#c792ea"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#addb67"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#82aaff"
            }
        }
    ]
}