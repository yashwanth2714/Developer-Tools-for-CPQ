{
    "$schema": "vscode://schemas/color-theme",
    "name": "Nord (CPQ)",
    "type": "dark",
    "colors": {
        "editor.background": "#2e3440",
        "editor.foreground": "#d8dee9",
        "editorCursor.foreground": "#88c0d0",
        "editor.lineHighlightBackground": "#3b4252",
        "editor.selectionBackground": "#434c5ecc"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#616e88"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#a3be8c"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#81a1c1"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#d08770"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#88c0d0"
            }
        }
    ]
}