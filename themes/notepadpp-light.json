{
    "$schema": "vscode://schemas/color-theme",
    "name": "Notepad++ Light (CPQ)",
    "type": "light",
    "colors": {
        "editor.background": "#ffffff",
        "editor.foreground": "#000000",
        "editorCursor.foreground": "#000000",
        "editor.lineHighlightBackground": "#f0f0f0",
        "editor.selectionBackground": "#cce8ff"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#008000",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#a31515"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#0000ff"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#001080"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#795e26"
            }
        },
        {
            "scope": "number",
            "settings": {
                "foreground": "#098658"
            }
        }
    ]
}