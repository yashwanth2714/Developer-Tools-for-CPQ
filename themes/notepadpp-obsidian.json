{
    "$schema": "vscode://schemas/color-theme",
    "name": "Notepad++ Obsidian (CPQ)",
    "type": "dark",
    "colors": {
        "editor.background": "#282b2e",
        "editor.foreground": "#e0e2e4",
        "editorCursor.foreground": "#ffffff",
        "editor.lineHighlightBackground": "#333639",
        "editor.selectionBackground": "#404447"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#7f9f7f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#cc9393"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#8cd0d3"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#dcdccc"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#efef8f"
            }
        },
        {
            "scope": "number",
            "settings": {
                "foreground": "#f0dfaf"
            }
        }
    ]
}