{
    "$schema": "vscode://schemas/color-theme",
    "name": "Palenight (CPQ)",
    "type": "dark",
    "colors": {
        "editor.background": "#292d3e",
        "editor.foreground": "#a6accd",
        "editorCursor.foreground": "#ffcc00",
        "editor.lineHighlightBackground": "#444267",
        "editor.selectionBackground": "#717cb470"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#676e95"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#c3e88d"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#c792ea"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#f78c6c"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#82aaff"
            }
        }
    ]
}