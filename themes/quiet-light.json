{
    "$schema": "vscode://schemas/color-theme",
    "name": "Quiet Light (CPQ)",
    "type": "light",
    "colors": {
        "editor.background": "#f5f5f5",
        "editor.foreground": "#5a5a5a",
        "editorCursor.foreground": "#000000",
        "editor.lineHighlightBackground": "#eaeaea",
        "editor.selectionBackground": "#dcdcdc"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#999988",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#dd1144"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#445588"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#008080"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#990073"
            }
        }
    ]
}