{
    "$schema": "vscode://schemas/color-theme",
    "name": "Tomorrow Night Blue (CPQ)",
    "type": "dark",
    "colors": {
        "editor.background": "#002451",
        "editor.foreground": "#ffffff",
        "editorCursor.foreground": "#ffffff",
        "editor.lineHighlightBackground": "#00346e",
        "editor.selectionBackground": "#003f8e"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#7285b7"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#99cc99"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#ff9da4"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#ffc58f"
            }
        },
        {
            "scope": "function",
            "settings": {
                "foreground": "#bbdaff"
            }
        }
    ]
}